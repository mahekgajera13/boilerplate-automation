{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "dom"],
    "module": "Node16",                
    "moduleResolution": "Node16",      
    "types": ["cypress", "node"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": false,
    "baseUrl": "."
  },
  "include": ["cypress/**/*.ts", "cypress.config.ts", "global.d.ts"]
}
